	<creationDate>November 2008</creationDate>
	<author>Dioscouri Design</author>
	<authorEmail>info@dioscouri.com</authorEmail>
	<authorUrl>http://www.dioscouri.com</authorUrl>
	<copyright>2008 Dioscouri Design</copyright>
	<license>GNU/GPL</license>
	<version>1.5</version>
	<description>This module shows the main admin navigation module</description>

	<files>
		<filename module="mod_menujuga">mod_menujuga.php</filename>
		<filename>helper.php</filename>
		<filename>index.html</filename>
