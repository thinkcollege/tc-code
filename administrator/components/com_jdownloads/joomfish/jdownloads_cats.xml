<?xml version="1.0" ?>
<joomfish type="contentelement">
<name>jDownloads Categories</name>
<author>Arno Betz</author>
<version>1.4 beta</version>
<description>Definition for the jDownloads component - Categories</description>
<reference>
<table name="jdownloads_cats">
<field type="referenceid" name="cat_id" translate="0">ID</field>
<field type="titletext" name="cat_title" translate="1">Title</field>
<field type="textarea" name="metadesc" translate="1">Meta Description</field>
<field type="textarea" name="metakey" translate="1">Meta Keys</field>
<field type="htmltext" name="cat_description" translate="1">Description</field>
<field type="checked_out" name="checked_out" translate="0">Check out</field>
<field type="checked_out_date" name="checked_out_time" translate="0">Check out date</field>
</table>
</reference>
</joomfish>
